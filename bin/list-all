#!/usr/bin/env bash

# curl -s https://dl.bintray.com/sbt/native-packages/sbt/ | \
#     grep 'href=' | \
#     awk -F'>' '{print $3}' | awk -F'<' '{print $1}' | sed -e 's/\///g' | \
#     sed -e 's/\n/ /g' | tr '\n' ' '

# use github api
curl -Fs https://api.github.com/repos/sbt/sbt/tags | \
    grep '"name":' | \
    awk -F'"' '{print $4}' | \
    cut -d2- | \
    grep -v '-' | \
    tr '\n' ' '

